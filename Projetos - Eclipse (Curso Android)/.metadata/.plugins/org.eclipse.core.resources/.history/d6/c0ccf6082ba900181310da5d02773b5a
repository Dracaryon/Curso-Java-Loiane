package com.loiane.cursojava.aula34exerc.variav_e_method_static.exerc4;

public class ConversaoDeUnidadesDeArea {
	
	double metroQuadrado = 10.76 * conversPeQuadrado(1);
	double peQuadrado = 929; //centímetros
	double milhaQuadrada;
	double Acre;

	static double resultado;

	double conversMetroQuadrado(double num1) {
		resultado = conversPeQuadrado(num1);
		return resultado;
	}

	double conversPeQuadrado(double num1) {
		resultado = num1 * peQuadrado; 
		return resultado;
	}

	double conversMilhaQuadrada(double num1) {
		return resultado;
	}

	double conversAcre(double num1) {
		return resultado;

	}
}
