package com.loiane.cursojava.aula34exerc.variav_e_method_static.exerc4;

public class TesteConversao {

	public static void main(String[] args) {
		
		System.out.println(	"\npeQuadrado: " + ConversaoDeUnidadesDeArea.peQuadrado);
